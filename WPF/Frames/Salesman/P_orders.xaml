<Page x:Class="WPF.Frames.Manager.P_orders"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF.Frames.Manager"
            xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="P_orders">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
            <RowDefinition></RowDefinition>

        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
            <Button x:Name="b_prd_list"  Height="32" Margin="5 0 5 0" Click="B_to_prList"  >Список заказов</Button>
            <Button x:Name="b_prd_insert"  Height="32" Margin="5 0 5 0" Click="Bto_add_Order"  >Добавить заказ</Button>
        </StackPanel>
        <Frame x:Name="spase" Grid.Row="1">

        </Frame>
    </Grid>

</Page>
