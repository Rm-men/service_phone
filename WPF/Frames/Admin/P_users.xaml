<Page x:Class="WPF.Frames.Admin.P_users"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF.Frames.Admin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="P_users">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
            <RowDefinition></RowDefinition>

        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
            <Button x:Name="b_emp_list"  Height="32" Margin="5 0 5 0"  >Таблица клиентов</Button>
            <Button x:Name="b_emp_upade"  Height="32" Margin="5 0 5 0" >Изменить параметры клиента</Button>
            <Button x:Name="b_emp_delete"  Height="32" Margin="5 0 5 0">Удалить клиента</Button>
        </StackPanel>
        <Frame x:Name="spase" Grid.Row="1">

        </Frame>

    </Grid>
</Page>
